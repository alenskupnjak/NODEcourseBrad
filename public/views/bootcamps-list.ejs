<% for (let data of bootcamps) {%>
	<div class="card mb-3">
		<div class="row no-gutters">
			<div class="col-md-4">
				<img src="/img/image_1.jpg" class="card-img" alt="..." />
			</div>
			<div class="col-md-8">
				<div class="card-body">
					<h5 class="card-title">
						<a href="/api/v1/bootcamps/<%= data._id%>"><%= data.name%>
							<span class="float-right badge badge-success">8.8</span></a>
					</h5>
					<span class="badge badge-dark mb-2"><%= data.location.city%>,<%= data.location.state%></span>
					<!-- <span class="badge badge-dark mb-2">Boston, MA</span> -->
					<p class="card-text">
						Web Development, UI/UX, Mobile Development
					</p>
				</div>
			</div>
		</div>
	</div>
	<% } %>

